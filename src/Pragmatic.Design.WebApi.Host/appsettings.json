{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.ApplicationInsights", "Serilog.Sinks.File", "Serilog.Sinks.Console", "Serilog.Sinks.Email", "Serilog.Sinks.Slack" ],
    "MinimumLevel": "Debug"

    //"WriteTo": [
    //  {
    //    "Name": "ApplicationInsights",
    //    "Args": {
    //      "connectionString": "[your connection string here]",
    //      "telemetryConverter": "Serilog.Sinks.ApplicationInsights.TelemetryConverters.TraceTelemetryConverter, Serilog.Sinks.ApplicationInsights"
    //    }
    //  }
    //]
  },
  "CORS": {
    "AllowSpecificOrigins": false,
    "AllowedOrigins": [],
    //Example origins
    //AllowedOrigins": [ "https://example.com", "https://api.example.com" ],
    "AllowSpecificHeaders": false,
    "AllowedHeaders": [ "Content-Type", "Authorization" ],
    //Example headers
    //"AllowedHeaders": [ "Content-Type", "Authorization" ],
    "AllowSpecificMethods": false,
    "AllowedMethods": []
    //Example methods
    //"AllowedMethods": [ "GET", "POST", "PUT" ]
  },
  "AllowedHosts": "*",
  "AzureFileLoggerOptions": {
    "FileName": "azure-diagnostics-",
    "FileSizeLimit": 51200,
    "RetainedFileCountLimit": 5
  },
  "AzureBlobLoggerOptions": {
    "BlobName": "log.txt"
  },
  "FeatureManagement": {
    "FeatureT": {
      "EnabledFor": [
        {
          "Name": "AlwaysOn"
        }
      ]
    },
    "FeatureU": {
      "EnabledFor": []
    },
    "FeatureV": {
      "EnabledFor": [
        {
          "Name": "TimeWindow",
          "Parameters": {
            "Start": "Wed, 01 May 2019 13:59:59 GMT",
            "End": "Mon, 01 July 2019 00:00:00 GMT"
          }
        }
      ]
    }
  }
}
